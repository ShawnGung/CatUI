.cat table
    background-color transparent
    border-collapse collapse
    border-spacing 0
    empty-cells hide
    
.cat th td
    text-align left
    border-bottom 1px solid #111
    
.cat thead th
    vertical-align bottom
    
.cat thead td
    vertical-align top
    border-bottom 1px #eaeaea solid
    
.cat tbody tr:last-child td
  border-bottom 0